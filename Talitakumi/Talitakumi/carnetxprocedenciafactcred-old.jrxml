<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Estadistica de Actividades" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="30" rightMargin="30" topMargin="20" bottomMargin="20">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="fechadesde" class="java.util.Date"/>
	<parameter name="fechahasta" class="java.util.Date"/>
	<parameter name="clientedesde" class="java.lang.Integer"/>
	<parameter name="clientehasta" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select episodios.cliente as idcli, clientes.nombre, fecha, documento, apellidos, nombres, facturas.tipo as tipofact,
       facturas.numero, actividades.descripcion as descact
from
	carnetdesalud, clientes, episodios, personas, facturas, actividades
where
        episodios.cliente = clientes.id     and
        carnetdesalud.episodio=episodios.id and
        episodios.paciente = personas.id    and
	facturas.episodio = episodios.id    and
        fecha >= $P{fechadesde}             and
        fecha <= $P{fechahasta}             and
	facturas.tipo = 'R'                 and
	actividades.id	= episodios.tipoepisodio and
	entregado='S'                            and
	episodios.cliente >= $P{clientedesde}    and
	episodios.cliente <= $P{clientehasta}
        GROUP BY clientes.nombre, episodios.fecha, episodios.paciente
        order by clientes.nombre, actividades.descripcion, fecha, apellidos]]>
	</queryString>
	<field name="idcli" class="java.lang.Integer"/>
	<field name="nombre" class="java.lang.String"/>
	<field name="fecha" class="java.sql.Date"/>
	<field name="documento" class="java.lang.Integer"/>
	<field name="apellidos" class="java.lang.String"/>
	<field name="nombres" class="java.lang.String"/>
	<field name="tipofact" class="java.lang.String"/>
	<field name="numero" class="java.lang.Integer"/>
	<field name="descact" class="java.lang.String"/>
	<variable name="cuenta_lineas" class="java.lang.Integer" resetType="Column" calculation="Count">
		<variableExpression><![CDATA[$F{documento}]]></variableExpression>
	</variable>
	<variable name="cuentaCarnet" class="java.lang.Integer" calculation="Count">
		<variableExpression><![CDATA[$F{documento}]]></variableExpression>
	</variable>
	<variable name="totalcliente" class="java.lang.Integer" resetType="Group" resetGroup="academia" calculation="Count">
		<variableExpression><![CDATA[$F{documento}]]></variableExpression>
	</variable>
	<variable name="cantactividad" class="java.lang.Integer" resetType="Group" resetGroup="actividad" calculation="Count">
		<variableExpression><![CDATA[$F{descact}]]></variableExpression>
	</variable>
	<group name="academia" isReprintHeaderOnEachPage="true">
		<groupExpression><![CDATA[($F{nombre})]]></groupExpression>
