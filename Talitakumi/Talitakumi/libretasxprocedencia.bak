	<parameter name="fechahasta" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select procedencia, descripcion, fecha, documento, apellidos, nombres 
from 
	libretasdeconducir, procedencias, episodios,personas 
where 
        procedencia = procedencias.id and 
        episodio=episodios.id         and 
        episodios.paciente = personas.id and 
        procedencia !=0 and 
        fecha >=$P{fechadesde} and 
        fecha <= $P{fechahasta} 
        GROUP BY procedencias.descripcion, 
                 episodios.fecha, episodios.paciente 
        order by procedencias.descripcion, fecha, apellidos]]></queryString>

