<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="ResumenDeCajaEfectivo" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="30" rightMargin="30" topMargin="20" bottomMargin="20">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="fecha_desde" class="java.lang.String"/>
	<parameter name="fecha_hasta" class="java.lang.String"/>
	<parameter name="usuario" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	tipodeactividad.id as idact,  tipodeactividad.descripcion as descact, facturas.tipo, count(*) as cantidad, sum(importe - descuentos) as pesos, usuarios.usuario as nombrete
FROM
	episodios, facturas, tipodeactividad, usuarios
WHERE
     episodios.id = facturas.episodio
 AND episodios.tipoepisodio = tipodeactividad.id
 AND fechaentregado >= $P{fecha_desde}
 AND fechaentregado <= $P{fecha_hasta}
 and entregado = 'S' 
 and (facturas.tipo='C' or facturas.tipo='R')
 and facturas.funcionario = $P{usuario}
 and usuarios.funcionario = facturas.funcionario
group by
	tipodeactividad.descripcion, facturas.tipo
order by 
	tipodeactividad.descripcion,
	facturas.tipo]]>
	</queryString>
	<field name="descact" class="java.lang.String"/>
	<field name="tipo" class="java.lang.String"/>
	<field name="cantidad" class="java.lang.Long"/>
	<field name="pesos" class="java.lang.Float"/>
	<field name="idact" class="java.lang.Integer"/>
	<field name="nombrete" class="java.lang.String"/>
	<variable name="tipopago" class="java.lang.String">
		<variableExpression><![CDATA[$F{tipo}.toString().equals("C") ? new java.lang.String("CONTADO") : new java.lang.String("CREDITO")]]></variableExpression>
	</variable>
	<variable name="contado" class="java.lang.Long" resetType="Group" resetGroup="tipopago" calculation="Sum">
		<variableExpression><![CDATA[$F{tipo}.toString().equals("C") ? $F{cantidad} : new java.lang.Long(0)]]></variableExpression>
	</variable>
	<variable name="credito" class="java.lang.Long" resetType="Group" resetGroup="tipopago" calculation="Sum">
		<variableExpression><![CDATA[$F{tipo}.toString().equals("R") ? $F{cantidad} : new java.lang.Long(0)]]></variableExpression>
	</variable>
	<variable name="sumacontado" class="java.lang.Long" resetType="Group" resetGroup="actividad" calculation="Sum">
		<variableExpression><![CDATA[$V{contado}]]></variableExpression>
	</variable>
	<variable name="sumacredito" class="java.lang.Long" resetType="Group" resetGroup="actividad" calculation="Sum">
		<variableExpression><![CDATA[$V{credito}]]></variableExpression>
	</variable>
	<variable name="sumapesos" class="java.lang.Float" resetType="Group" resetGroup="actividad" calculation="Sum">
		<variableExpression><![CDATA[$F{pesos}]]></variableExpression>
	</variable>
	<variable name="totalpesos" class="java.lang.Float" resetType="Group" resetGroup="tipopago" calculation="Sum">
		<variableExpression><![CDATA[$V{sumapesos}]]></variableExpression>
	</variable>
	<variable name="toalcontado" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$V{contado}]]></variableExpression>
	</variable>
	<variable name="totalcredito" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$V{credito}]]></variableExpression>
	</variable>
	<variable name="totaldinero" class="java.lang.Float" calculation="Sum">
		<variableExpression><![CDATA[$F{tipo}.toString().equals("C") ? $F{pesos} : new java.lang.Float(0)]]></variableExpression>
	</variable>
	<variable name="pesoscontado" class="java.lang.Float" resetType="Group" resetGroup="actividad" calculation="Sum">
		<variableExpression><![CDATA[$F{tipo}.toString().equals("C") ? $F{pesos} : new java.lang.Float(0)]]></variableExpression>
	</variable>
	<variable name="totalactividad" class="java.lang.Integer" calculation="Sum"/>
	<group name="actividad">
		<groupExpression><![CDATA[$F{idact}]]></groupExpression>
		<groupHeader>
			<band/>
		</groupHeader>
		<groupFooter>
			<band height="20">
				<rectangle>
					<reportElement x="0" y="0" width="535" height="20"/>
