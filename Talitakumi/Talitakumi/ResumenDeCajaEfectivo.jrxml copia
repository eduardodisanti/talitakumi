			</line>
			<textField isStretchWithOverflow="true" pattern="dd/MM/yyyy h.mm a">
				<reportElement x="0" y="52" width="157" height="12"/>
				<textElement/>
				<textFieldExpression class="java.util.Date"><![CDATA[new Date()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="418" y="19" width="117" height="20"/>
				<textElement>
					<font isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{nombrete}]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
