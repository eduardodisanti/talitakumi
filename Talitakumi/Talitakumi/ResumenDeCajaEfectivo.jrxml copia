					<reportElement x="347" y="0" width="1" height="20"/>
				</line>
				<line>
					<reportElement x="425" y="0" width="1" height="20"/>
				</line>
				<textField isBlankWhenNull="true">
					<reportElement x="189" y="1" width="63" height="19"/>
					<textElement textAlignment="Right" verticalAlignment="Middle">
						<font isBold="true" isItalic="true"/>
					</textElement>
					<textFieldExpression class="java.lang.Long"><![CDATA[new java.lang.Long($V{sumacredito}.longValue() + $V{sumacontado}.longValue())]]></textFieldExpression>
				</textField>
				<textField pattern="#,##0.00;(-#,##0.00)">
					<reportElement x="428" y="1" width="105" height="19"/>
					<textElement textAlignment="Right" verticalAlignment="Middle"/>
					<textFieldExpression class="java.lang.Float"><![CDATA[$V{pesoscontado}]]></textFieldExpression>
				</textField>
				<textField>
					<reportElement x="253" y="1" width="92" height="19"/>
					<textElement textAlignment="Right" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<textFieldExpression class="java.lang.Long"><![CDATA[$V{sumacontado}]]></textFieldExpression>
				</textField>
				<textField isBlankWhenNull="false">
					<reportElement key="textField" x="0" y="1" width="189" height="19"/>
					<textElement verticalAlignment="Middle">
						<font size="9"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA[$F{descact}]]></textFieldExpression>
				</textField>
				<textField>
					<reportElement x="344" y="1" width="81" height="19"/>
					<textElement textAlignment="Right" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<textFieldExpression class="java.lang.Long"><![CDATA[$V{sumacredito}]]></textFieldExpression>
				</textField>
			</band>
		</groupFooter>
	</group>
	<group name="tipopago">
		<groupExpression><![CDATA[$F{tipo}]]></groupExpression>
		<groupHeader>
			<band height="50"/>
		</groupHeader>
		<groupFooter>
			<band height="50"/>
		</groupFooter>
	</group>
	<background>
		<band/>
	</background>
	<title>
		<band/>
	</title>
	<pageHeader>
		<band height="150">
			<staticText>
				<reportElement key="staticText-1" x="358" y="19" width="60" height="20"/>
				<textElement/>
				<text><![CDATA[Funcionario]]></text>
			</staticText>
			<staticText>
				<reportElement key="staticText-2" x="170" y="0" width="188" height="15"/>
				<box>
					<topPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<leftPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<bottomPen lineWidth="0.0" lineColor="#000000"/>
					<rightPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
