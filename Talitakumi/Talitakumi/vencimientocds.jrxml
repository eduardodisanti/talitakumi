	<queryString>
		<![CDATA[SELECT
     personas.documento AS episodios_paciente,
     episodios.`fecha` AS episodios_fecha,
     carnetdesalud.`vigenciamedico` AS carnetdesalud_vigenciamedico,
     carnetdesalud.`vigencialaboratorio` AS carnetdesalud_vigencialaboratorio,
     carnetdesalud.`vigenciaodontologo` AS carnetdesalud_vigenciaodontologo,
     personas.`apellidos` AS personas_apellidos,
     personas.`nombres` AS personas_nombres,
     personas.`documento` AS personas_documento,
     personas.`telefono` AS personas_telefono,
     personas.`movil` AS personas_movil,
     date_add(`fecha`, interval `vigenciamedico` MONTH) as vence,
     clientes.nombre as nomcli,
     month(date_add(`fecha`, interval `vigenciamedico` MONTH)),
     year(date_add(`fecha`, interval `vigenciamedico` MONTH))
FROM
     `episodios`, carnetdesalud, personas, clientes
where
      episodios.paciente = personas.id and
      episodios.id=episodio and
      episodios.cliente = clientes.id and
      fecha >= $P{fecha_desde} and
      vigenciamedico > 0 and
      date_add(`fecha`, interval `vigenciamedico` MONTH) >= $P{fecha_hoy} and
      date_add(`fecha`, interval `vigenciamedico` MONTH) <= $P{fecha_hasta}
order by  episodios.cliente, vence, apellidos, nombres]]>
