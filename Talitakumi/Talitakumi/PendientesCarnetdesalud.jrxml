	<queryString>
		<![CDATA[select personas.documento,clientes.nombre as clientenombre,clientes.id as clienteid, personas.nombres, personas.apellidos, episodio, carnetdesalud.vigencialaboratorio, vigenciaodontologo, vigenciamedico
from personas, carnetdesalud, episodios, clientes
where
 episodios.cliente = clientes.id and
 episodios.paciente = personas.id and
 carnetdesalud.episodio = episodios.id and
clientes.id = carnetdesalud.cliente and (
 carnetdesalud.vigencialaboratorio= -1 OR
 carnetdesalud.vigenciaodontologo= -1 OR
 carnetdesalud.vigenciamedico = -1 OR
 carnetdesalud.episodio not in
     (select episodiosvacunas.episodio from episodiosvacunas where episodiosvacunas.episodio=carnetdesalud.episodio and vacuna = 1 )
OR
 carnetdesalud.episodio not in
     (select episodiosestudios.episodio from episodiosestudios where personas.sexo='F' and episodiosestudios.episodio=carnetdesalud.episodio and (estudio = 21 or estudio = 22) )

) and episodios.fecha >= $P{fechaDesde} and episodios.fecha<=$P{fechaHasta} and clientes.id >= $P{clienteDesde} and clientes.id <= $P{clienteHasta}

group by episodio
order by personas.documento]]>
