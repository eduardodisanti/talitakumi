	<parameter name="fechadesde" isForPrompting="true" class="java.util.Date"/>
	<parameter name="fechahasta" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT
     nombre,
     fecha,
     documento,
     apellidos,
     nombres,
     cliente,
     libretasdeconducir.`episodio` AS libretasdeconducir_episodio,
     episodios.`id` AS episodios_id
FROM
     `libretasdeconducir` libretasdeconducir,
     `episodios` episodios,
     `personas` personas,
     `clientes` clientes
WHERE
     cliente = clientes.id
     and episodio = episodios.id
     and episodios.paciente = personas.id
     and fecha >= $P{fechadesde}
     and fecha <= $P{fechahasta}
     and episodios.cliente >1
GROUP BY
     clientes.nombre,
     episodios.fecha,
     episodios.paciente
ORDER BY
     clientes.nombre ASC,
     fecha ASC,
     apellidos ASC]]></queryString>

