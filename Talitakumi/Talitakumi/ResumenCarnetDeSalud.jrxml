	<queryString>
		<![CDATA[SELECT
     descripcion,
     facturas.tipo,
     facturas.`importe`,
     count(*)as cantidad,
     sum(importe - descuentos)as dinero
FROM
     `facturas` facturas,
     `episodios` episodios,
     `actividades` actividades
WHERE
     episodios.id = facturas.episodio
     and actividades.id = episodios.tipoepisodio
     and episodios.fecha >='2009-09-01' and episodios.fecha <= '2009-09-31'
     and facturas.entregado = 'S'
GROUP BY
     facturas.tipo,
     actividades.descripcion,
     facturas.importe]]>
	</queryString>
