				<textField pattern="dd/MM/yyyy" isBlankWhenNull="false">
					<reportElement key="textField" x="430" y="0" width="100" height="20"/>
					<textElement/>
					<textFieldExpression class="java.util.Date"><![CDATA[$P{fechadesde}]]></textFieldExpression>
				</textField>
				<textField pattern="dd/MM/yyyy" isBlankWhenNull="false">
					<reportElement key="textField" x="430" y="19" width="100" height="20"/>
