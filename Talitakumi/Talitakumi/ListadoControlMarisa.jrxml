	<queryString>
		<![CDATA[SELECT
     actividades.`descripcion` AS descact,
     count(*)as cantidad,
     agenda.`fecha` AS agenda_fecha,
     agenda.`hora` AS agenda_hora,
     episodios.`cliente` AS episodios_cliente,
     clientes.`nombre` AS clientes_nombre
FROM
     `episodios` episodios INNER JOIN `agenda` agenda ON episodios.`id` = agenda.`episodio`
     INNER JOIN `clientes` clientes ON episodios.`cliente` = clientes.`id`
     INNER JOIN `actividades` actividades ON episodios.`tipoepisodio` = actividades.`id`
WHERE
     episodios.fecha >= $P{fecha_desde}
     and episodios.fecha <= $P{fecha_hasta}
GROUP BY
     actividades.descripcion,
     clientes.nombre,
     agenda.fecha,
     agenda.hora]]>
	</queryString>
