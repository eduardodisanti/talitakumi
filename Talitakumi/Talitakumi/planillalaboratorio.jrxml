from
	analisis, analisishechos, estudios, personas, episodios, estudioshechos
where
        episodios.fecha = $P{fechahoy} and episodios.paciente = personas.id and
        estudioshechos.episodio = episodios.id and analisishechos.estudiohecho = estudioshechos.id and
        estudioshechos.estudio = estudios.id and analisishechos.analisis = analisis.id
group by
	episodios.id, personas.apellidos, personas.nombres, personas.sexo, personas.fechanacimiento, estudios.descripcion, analisis.id, analisishechos.valorhallado
