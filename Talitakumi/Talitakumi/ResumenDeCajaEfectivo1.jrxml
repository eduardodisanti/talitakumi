	<queryString>
		<![CDATA[SELECT
	tipodeactividad.descripcion as tipoact, actividades.descripcion, facturas.tipo, count(*) as cantidad, sum(importe - descuentos) as pesos
FROM
	episodios, facturas, actividades, tipodeactividad
WHERE
     episodios.id = facturas.episodio
 AND actividades.tipoactividad = tipodeactividad.id
 AND episodios.tipoepisodio = actividades.id
 AND fechaentregado >= $P{fecha_desde}
 AND fechaentregado <= $P{fecha_hasta}
 and entregado = 'S' 
 and (facturas.tipo='C' or facturas.tipo='R')
 and funcionario = $P{usuario}
group by
	tipodeactividad.descripcion, actividades.descripcion, facturas.tipo
order by 
	tipodeactividad.descripcion, actividades.descripcion,
	facturas.tipo]]>
	</queryString>
