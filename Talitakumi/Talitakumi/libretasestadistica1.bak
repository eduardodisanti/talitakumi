	<parameter name="fechadesde" isForPrompting="true" class="java.util.Date"/>
	<parameter name="fechahasta" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select procedencia, descripcion, fecha, facturas.tipo as tipofact,
       facturas.numero, libretasdeconducir.tipo as tipolib, count(*) as cant
from 
	libretasdeconducir, procedencias, episodios, facturas 
where 
        procedencia = procedencias.id and 
        libretasdeconducir.episodio=episodios.id and 
	facturas.episodio = episodios.id and
        fecha >= $P{fechadesde} and 
        fecha <= $P{fechahasta} and
        entregado='S'
        GROUP BY procedencias.descripcion, libretasdeconducir.tipo                
        order by procedencias.descripcion, fecha]]></queryString>

