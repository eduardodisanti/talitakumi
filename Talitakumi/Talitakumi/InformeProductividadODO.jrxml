	<queryString>
		<![CDATA[select medicos.nombre, tipodeactividad.descripcion, count(*) as cantidad
from
	episodios, episodioodontologo, medicos, actividades, tipodeactividad
where
	episodioodontologo.medico = medicos.codigo and fecha>=$P{fechadesde} and fecha<=$P{fechahasta} and episodioodontologo.medico!=0 and
        episodios.tipoepisodio = actividades.id and tipodeactividad.id = actividades.tipoactividad and
	episodios.id = episodioodontologo.episodio
group by
	episodioodontologo.medico, tipodeactividad.descripcion;]]>
	</queryString>
