		<![CDATA[SELECT
     count(*) AS cuenta_actividades,
     sum(facturas.`importe`) AS facturas_importe,
     sum(facturas.`descuentos`) AS facturas_descuentos,
facturas.emisor as emidorid, emisor.nombre as nombreemisor
FROM
    facturas, episodios, actividades, emisor
WHERE
          ((emisor.id = $P{emisor}) or ($P{emisor}=0)) and
          facturas.emisor = emisor.id and
          facturas.episodio = episodios.id
 AND episodios.tipoepisodio = actividades.id
 AND fechaentregado >= $P{fechadesde}
 AND fechaentregado <= $P{fechahasta}
 AND entregado = 'S'
 AND tipo = 'C'
group by facturas.emisor]]>
