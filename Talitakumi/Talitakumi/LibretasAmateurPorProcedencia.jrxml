procedencias.descripcion,libretasdeconducir.procedencia,count(*), sum(facturas.importe-facturas.descuentos), facturas.tipo, facturas.emisor as emisor

from

libretasdeconducir, facturas, actividades, procedencias, episodios

where

libretasdeconducir.procedencia = procedencias.id and facturas.episodio = episodios.id and episodios.tipoepisodio = actividades.id
and episodios.id = libretasdeconducir.episodio and ((facturas.tipo=$P{tipo_factura}) or ($P{tipo_factura}='T'))  and (facturas.tipo = 'C' or facturas.tipo='R') and episodios.fecha >= $P{fechadesde} and episodios.fecha<=$P{fechahasta}
group by procedencias.descripcion, facturas.tipo
order by libretasdeconducir.procedencia, facturas.tipo]]>
