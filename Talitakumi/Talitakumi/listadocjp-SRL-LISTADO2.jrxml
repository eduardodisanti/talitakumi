	<queryString>
		<![CDATA[SELECT
     documento,
     apellidos,
     nombres,
     tipodeactividad.`descripcion`,
     episodios.`fecha`,
     facturas.`numero`,
     facturas.`emisor`
FROM
     facturas, episodios, actividades, tipodeactividad, personas
WHERE
     fecha >= $P{fecha_desde}
 AND fecha <= $P{fecha_hasta}
 and entregado = 'S'
 and episodios.id = facturas.episodio
 and (facturas.emisor = $P{emisor} OR facturas.emisor = 1)
 and actividades.tipoactividad = tipodeactividad.id
 and episodios.tipoepisodio = actividades.id
 and personas.id = paciente
 and actividades.tipoactividad = 10
ORDER BY
     facturas.emisor ASC,
     facturas.numero DESC,
     personas.id ASC,
     fechaentregado ASC,
     actividades.tipoactividad ASC
LIMIT $P{limite}]]>
	</queryString>
