	<parameter name="fechadesde" class="java.util.Date"/>
	<parameter name="fechahasta" class="java.util.Date"/>
	<parameter name="clientedesde" class="java.lang.Integer"/>
	<parameter name="clientehasta" class="java.lang.Integer"/>
	<parameter name="academiadesde" class="java.lang.Integer"/>
	<parameter name="academiahasta" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     nombre,  fecha, personas.documento,   apellidos,  nombres, cliente,
     libretasdeconducir.`episodio` AS libretasdeconducir_episodio,
     episodios.`id` AS episodios_id,  facturas.tipo
FROM
      libretasdeconducir,  episodios,  personas, clientes, facturas
WHERE
          cliente = clientes.id
     and libretasdeconducir.episodio = episodios.id
     and episodios.paciente = personas.id
     and fecha >= $P{fechadesde}
     and fecha <= $P{fechahasta}
     and episodios.cliente > 0
     and facturas.numero != 0
     and facturas.episodio = episodios.id
     and episodios.cliente >= $P{clientedesde}
     and episodios.cliente <= $P{clientehasta}
     and libretasdeconducir.procedencia >= $P{academiadesde}
     and libretasdeconducir.procedencia <= $P{academiahasta}
GROUP BY
     clientes.nombre,
     episodios.fecha,
     episodios.paciente
ORDER BY
     clientes.nombre ASC,
     fecha ASC,
     apellidos ASC]]>
	</queryString>
