	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="fecha_desde" class="java.lang.String"/>
	<parameter name="fecha_hasta" class="java.lang.String"/>
	<parameter name="usuario" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	tipodeactividad.id as idact,  tipodeactividad.descripcion as descact, facturas.tipo, count(*) as cantidad, sum(importe - descuentos) as pesos, usuarios.usuario as nombrete, usuarios.funcionario as fun
FROM
	episodios, facturas, tipodeactividad, usuarios
WHERE
     episodios.id = facturas.episodio
 AND episodios.tipoepisodio = tipodeactividad.id
 AND fechaentregado >= $P{fecha_desde}
 AND fechaentregado <= $P{fecha_hasta}
 and entregado = 'S' 
 and (facturas.tipo='C' or facturas.tipo='R')
 and usuarios.funcionario = facturas.funcionario
group by
	tipodeactividad.descripcion, facturas.tipo
order by 
	tipodeactividad.descripcion,
	facturas.tipo]]>
	</queryString>
