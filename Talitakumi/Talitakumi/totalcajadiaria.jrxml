	<variable name="totaldinero" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{tipo}.toString().equals("C") ? $F{pesos} : new java.lang.Double(0)]]></variableExpression>
	</variable>
	<variable name="pesoscontado" class="java.lang.Double" resetType="Group" resetGroup="actividad" calculation="Sum">
		<variableExpression><![CDATA[$F{tipo}.toString().equals("C") ? $F{pesos} : new java.lang.Double(0)]]></variableExpression>
	</variable>
