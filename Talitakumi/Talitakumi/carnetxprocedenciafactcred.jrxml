	<queryString>
		<![CDATA[select episodios.cliente as idcli, clientes.nombre, episodios.fecha, documento, apellidos, nombres, facturas.tipo as tipofact,
       facturas.numero,
       actividades.descripcion as descact,
       consultorios.sucursal as sucursal,
       facturas.importe, facturas.descuentos, (facturas.importe - facturas.descuentos) as cobrado
from
	carnetdesalud, clientes, episodios, personas, facturas, actividades, agenda, consultorios
where
        episodios.cliente = clientes.id     and
        carnetdesalud.episodio=episodios.id and
        episodios.paciente = personas.id    and
	facturas.episodio = episodios.id    and
        episodios.fecha >= $P{fechadesde}             and
        episodios.fecha <= $P{fechahasta}             and
	episodios.tipoepisodio = actividades.id    and
	agenda.episodio        = episodios.id      and
	agenda.consultorio     = consultorios.id   and
	episodios.cliente >= $P{clientedesde}      and
	episodios.cliente <= $P{clientehasta}      and
	facturas.tipo = 'R'                        and
        entregado='S'
        GROUP BY clientes.nombre, actividades.descripcion, sucursal, episodios.fecha, episodios.paciente, facturas.id
        order by clientes.nombre, actividades.descripcion, sucursal, episodios.fecha, apellidos]]>
