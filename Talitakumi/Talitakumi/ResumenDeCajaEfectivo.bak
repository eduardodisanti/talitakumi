	<parameter name="fecha_desde" isForPrompting="true" class="java.lang.String"/>
	<parameter name="fecha_hasta" isForPrompting="true" class="java.lang.String"/>
	<parameter name="usuario" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT
	actividades.descripcion, facturas.tipo, count(*) as cantidad
FROM
	episodios, facturas, actividades, tipodeactividad
WHERE
     episodios.id = facturas.episodio
 AND actividades.tipoactividad = tipodeactividad.id
 AND episodios.tipoepisodio = actividades.id
 AND fechaentregado >= $P{fecha_desde}
 AND fechaentregado <= $P{fecha_hasta}
 and entregado = 'S' 
 and funcionario = $P{usuario}
group by
	actividades.descripcion
order by 
	tipodeactividad.descripcion, 
	facturas.tipo,
        actividades.descripcion]]></queryString>

