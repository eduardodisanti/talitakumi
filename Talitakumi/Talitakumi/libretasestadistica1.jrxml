<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Estadistica de Actividades" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="30" rightMargin="30" topMargin="20" bottomMargin="20">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="fechadesde" class="java.util.Date"/>
	<parameter name="fechahasta" class="java.util.Date"/>
	<parameter name="clientedesde" class="java.lang.Integer"/>
	<parameter name="clientehasta" class="java.lang.Integer"/>
	<parameter name="academiadesde" class="java.lang.Integer"/>
	<parameter name="academiahasta" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select procedencia, descripcion, fecha, facturas.tipo as tipofact,
       facturas.numero, libretasdeconducir.tipo as tipolib, count(*) as cant
from
	libretasdeconducir, procedencias, episodios, facturas
where
        procedencia = procedencias.id and
        libretasdeconducir.episodio=episodios.id and
	facturas.episodio = episodios.id and
        fecha >= $P{fechadesde} and
        fecha <= $P{fechahasta}
     and episodios.cliente >= $P{clientedesde}
     and episodios.cliente <= $P{clientehasta}
     and entregado='S'
     and libretasdeconducir.procedencia >= $P{academiadesde}
     and libretasdeconducir.procedencia <= $P{academiahasta}

        GROUP BY procedencias.descripcion, libretasdeconducir.tipo
        order by procedencias.descripcion, fecha]]>
	</queryString>
	<field name="fecha" class="java.sql.Date"/>
	<field name="descripcion" class="java.lang.String"/>
	<field name="tipofact" class="java.lang.String"/>
	<field name="cant" class="java.lang.Long"/>
	<field name="procedencia" class="java.lang.Integer"/>
	<field name="numero" class="java.lang.Integer"/>
	<field name="tipolib" class="java.lang.Integer"/>
	<variable name="cuenta_lineas" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[$F{cant}]]></variableExpression>
	</variable>
	<variable name="tipolibreta" class="java.lang.String">
		<variableExpression><![CDATA[$F{tipolib}.toString().equals("1") ? new java.lang.String("ASPIRANTE") : new java.lang.String("RENOVACION")]]></variableExpression>
	</variable>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band splitType="Stretch"/>
	</title>
	<pageHeader>
