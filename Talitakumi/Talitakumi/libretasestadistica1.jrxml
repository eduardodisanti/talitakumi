				<reportElement key="textField" x="370" y="2" width="32" height="20"/>
				<textElement/>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{tipolib}]]></textFieldExpression>
			</textField>
			<line>
				<reportElement key="line-1" x="0" y="23" width="530" height="1" forecolor="#CCCCCC"/>
			</line>
		</band>
	</detail>
	<columnFooter>
