					<font size="14" isBold="true"/>
				</textElement>
				<text><![CDATA[ESTADISTICA LIBRETAS DE CONDUCIR]]></text>
			</staticText>
			<textField pattern="dd/MM/yyyy" isBlankWhenNull="false">
				<reportElement key="textField" x="430" y="0" width="100" height="20"/>
				<textElement/>
