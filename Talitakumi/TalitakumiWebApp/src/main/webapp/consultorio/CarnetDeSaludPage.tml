<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
    <head>
        <title>iTalitakumi</title>
    </head>
    <body bgcolor="#dddddd">
        <div id="branding">
           <table border='0' width="100%">
                          <tr>
                               <td align="left"><img src='../images/logo.jpg'  width='100px'/></td>
                               <td align="center"><h3>CARNET DE SALUD BASICO</h3></td>
                               <td align="right"><h4>${fecha}</h4></td>
                          </tr>
           </table>
        </div>
<div>

 <table border='0' width="100%" bgcolor="#dddddd">
    <form t:type="form" t:id="doc" style="border: medium solid gray" action="#" >
        <tr>
             <td colspan = "2">
             CEDULA <input t:type="TextField" t:id="documento" size="8" t:validate="required" t:mixins="zoneUpdater" zone="pacienteZone" event="cedulaChanged" clientEvent="blur"/>
             </td>
        </tr>
    </form>
</table>
<t:zone t:id="pacienteZone">
