                <table border='0' width="100%" bgcolor="#dddddd">
                        <tr>
                            <td>
                                APELLIDOS <input t:type="TextField" t:id="apellidos" size="40"/>
                            </td>
                            <td>
                                NOMBRES <input t:type="TextField" t:id="nombres" size="40"/>
                            </td>
                         </tr>
                        <tr>
                            <td>
                                FECHA DE NACIMIENTO <input t:type="DateField" t:id="fechanacimiento" t:format="dd/MM/yyyy"/>
                            </td>
                            <td>
                                SEXO <input t:type="TextField" t:id="sexo" size="1" t:validate="required"/>
                            </td>
                         </tr>
                        <tr>
                            <td>
                                FOTO <t:checkbox t:id="foto"/>
                            </td>
                            <td>
                                CLIENTE<select t:type="select" t:model="clientes" encoder="clientes" t:value="cliente"></select>
                            </td>
                         </tr>
		  </table>
